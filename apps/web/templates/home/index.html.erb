<div class="area-services background-color-warning color-white">
	<div id="features" class="container">
        <div class="page-header text-center">
          <h3 id="progress-basic">我们的服务</h3>
        <middle>Our Services</middle>
        </div>
        <% feature_rows.each do |feature_row| %>
          <div class="row text-center top-buffer">
            <% feature_row.each do |feature| %>
              <div class="col-lg-4">
                <span class="icon-container color-red-lighten-1">
                  <% if feature[:type] == :icon %>
                    <i class="icon fa <%= feature[:icon_content] %>"></i>
                  <% end %>
                  <% if feature[:type] == :text %>
                    <%= feature[:icon_content] %>
                  <% end %>
                </span>
                <h2><%= feature[:title] %></h2>
                <p><%= feature[:description] %></p>
              </div>
            <% end %>
          </div>
        <% end %>
</div>
</div>
